<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
               "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	   <bean id="authResponderBean" 
	       class="com.jbrown.web.ws.responder.AuthResponder" />
	   <bean id="geoResponderBean" 
	        class="com.jbrown.web.ws.responder.CountryInfoResponder" />
	   <bean id="mediaResponder" 
	        class="com.jbrown.web.ws.responder.MediaResponder" />
	    
		 <bean id="responderFactory" 
		       class="com.jbrown.web.ws.responder.ResponderFactory">
				<property name="responders">
				  <map>
					  <entry key="authResponder" value-ref="authResponderBean" />
					  <entry key="geoResponder" value-ref="geoResponderBean" />
					  <entry key="mediaResponder" value-ref="mediaResponder" />
				  </map>
				</property>
		 </bean>
</beans>
