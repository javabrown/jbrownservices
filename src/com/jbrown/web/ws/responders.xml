<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
               "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	 <bean id="authResponder" class="com.jbrown.web.ws.AuthResponder" />
	 
		<bean id="responderFactory" class=" com.jbrown.web.ws.ResponderFactory">
				<property name="responders">
						<map>
								<entry key="authResponder" value-ref="authResponder" />
						</map>
				</property>
		</bean>
</beans>
